{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:List*"
            ],
            "Resource": "arn:aws:s3:::bucketName*"
        },
	{
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": [
                "arn:aws:s3:::bucketName/*ParticipationRewardDatum/*",
		"arn:aws:s3:::bucketName/push-notifications/updates/*"
	    ]
        },
	{
            "Effect": "Allow",
            "Action": "s3:DeleteObject",
            "Resource": "arn:aws:s3:::bucketName/push-notifications/*
        }
    ]
}